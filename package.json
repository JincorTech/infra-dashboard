{
  "name": "secrettech-app-dashboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "nodemon -w ./src -e ts --exec ts-node src/bin/main",
    "start:test": "nodemon -w ./src -e ts --exec mocha -r ts-node/register src/**/*.spec.ts",
    "lint": "tslint 'src/**/*.ts'",
    "lintFix": "tslint --fix 'src/**/*.ts'",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "test:cover": "nyc mocha src/**/*.spec.ts",
    "build": "tsc -p tsconfig.build.json --outDir dist",
    "serve": "node dist/bin/main.js"
  },
  "nyc": {
    "exclude": [
      "src/**/*.spec.ts"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "cors": "2.8.4",
    "debug": "3.1.0",
    "decamelize": "2.0.0",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "express-basic-auth": "1.1.5",
    "express-bearer-token": "2.1.1",
    "express-cors": "0.0.3",
    "express-jwt": "5.3.1",
    "express-winston": "2.6.0",
    "helmet": "^3.12.1",
    "http-status": "1.2.0",
    "inversify": "4.13.0",
    "inversify-express-utils": "6.1.0",
    "joi": "13.4.0",
    "jsonwebtoken": "8.3.0",
    "lru-cache": "4.1.3",
    "mongodb": "3.1.1",
    "node-uuid": "1.4.8",
    "reflect-metadata": "0.1.12",
    "typeorm": "0.2.7",
    "web-request": "1.0.7",
    "web3-utils": "1.0.0-beta.34",
    "winston": "3.0.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/chai-as-promised": "7.1.0",
    "@types/chai-http": "3.0.5",
    "@types/debug": "0.0.30",
    "@types/express": "4.16.0",
    "@types/faker": "4.1.2",
    "@types/http-status": "0.2.30",
    "@types/joi": "13.4.0",
    "@types/jsonwebtoken": "7.2.8",
    "@types/mocha": "5.2.5",
    "@types/node": "10.5.2",
    "@types/node-uuid": "0.0.28",
    "@types/nsqjs": "0.9.0",
    "@types/winston": "2.3.9",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chai-http": "4.0.0",
    "faker": "4.1.0",
    "mocha": "5.2.0",
    "mocha-prepare": "0.1.0",
    "nodemon": "1.18.3",
    "nyc": "12.0.2",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "tslint-config-standard": "7.1.0",
    "typemoq": "2.1.0",
    "typescript": "2.9.2"
  }
}
